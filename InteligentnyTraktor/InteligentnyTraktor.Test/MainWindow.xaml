        <TextBox x:Name="textBoxEnterRow" HorizontalAlignment="Left" Height="23" Margin="834,194,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="41"/>
        <TextBox x:Name="textBoxEnterColumn" HorizontalAlignment="Left" Height="23" Margin="834,237,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="41"/>
