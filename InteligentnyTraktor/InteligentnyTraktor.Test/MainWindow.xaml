        <Button x:Name="buttonSow" Content="Zasiej" HorizontalAlignment="Left" Margin="800,360,0,0" VerticalAlignment="Top" Width="75" Click="buttonSow_Click"/>
        <Button x:Name="buttonFertilize" Content="Nawieź" HorizontalAlignment="Left" Margin="800,385,0,0" VerticalAlignment="Top" Width="75" Click="buttonFertilize_Click"/>
        <Button x:Name="buttonHarvest" Content="Zbierz plon" HorizontalAlignment="Left" Margin="800,410,0,0" VerticalAlignment="Top" Width="75" Click="buttonHarvest_Click"/>
        <Label x:Name="labelVelocity" Content="Label" HorizontalAlignment="Left" Margin="38,748,0,-5" VerticalAlignment="Top"/>
        <Label x:Name="labelDirection" Content="Label" HorizontalAlignment="Left" Margin="226,748,0,-5" VerticalAlignment="Top"/>
        <Label x:Name="labelPosition" Content="Label" HorizontalAlignment="Left" Margin="400,748,0,-5" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxEnterCommend" HorizontalAlignment="Left" Height="23" Margin="760,528,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="206"/>
        <Label Content="Wpisz polecenie" HorizontalAlignment="Left" Margin="760,502,0,0" VerticalAlignment="Top"/>
        <Button x:Name="ButtonDo" Content="wykonaj" HorizontalAlignment="Left" Margin="765,556,0,0" VerticalAlignment="Top" Width="75" Click="ButtonDo_Click"/>

