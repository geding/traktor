<Window x:Class="InteligentnyTraktor.Test.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="524" Width="662">
    <Grid>
        <Grid x:Name="gridField"  HorizontalAlignment="Left" Height="400" Margin="37,35,0,0" VerticalAlignment="Top" Width="400" ShowGridLines="True">
        </Grid>
        <Canvas x:Name="fieldCanvas" HorizontalAlignment="Left" Height="400" VerticalAlignment="Top" Width="400" Margin="37,35,0,0"/>
        <Button x:Name="buttonMoveTractor" Content="Przesuń" HorizontalAlignment="Left" Margin="486,68,0,0" VerticalAlignment="Top" Width="75" Click="buttonMoveTractor_Click"/>
        <TextBox x:Name="textBoxEnterRow" HorizontalAlignment="Left" Height="23" Margin="560,135,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="41"/>
        <TextBox x:Name="textBoxEnterColumn" HorizontalAlignment="Left" Height="23" Margin="560,178,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="41"/>
        <Label Content="wiersz" HorizontalAlignment="Left" Margin="486,135,0,0" VerticalAlignment="Top"/>
        <Label Content="kolumna" HorizontalAlignment="Left" Margin="486,175,0,0" VerticalAlignment="Top"/>
        <Button x:Name="buttonStop" Content="Stop" HorizontalAlignment="Left" Margin="580,68,0,0" VerticalAlignment="Top" Width="45" Click="buttonStop_Click"/>
        <Label x:Name="labelCommunication" Content="" HorizontalAlignment="Left" Margin="486,35,0,0" VerticalAlignment="Top" Width="139"/>
        <Button x:Name="buttonSow" Content="Zasiej" HorizontalAlignment="Left" Margin="486,233,0,0" VerticalAlignment="Top" Width="75" Click="buttonSow_Click"/>
        <Label x:Name="labelVelocity" Content="Label" HorizontalAlignment="Left" Margin="37,440,0,0" VerticalAlignment="Top"/>
        <Label x:Name="labelDirection" Content="Label" HorizontalAlignment="Left" Margin="225,440,0,0" VerticalAlignment="Top"/>
        <Label x:Name="labelPosition" Content="Label" HorizontalAlignment="Left" Margin="399,440,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
